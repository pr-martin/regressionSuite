// ----------------------------------------------------------------------------
// Regression Test for QuasiTest package
//
// 
// Executes the AGREE verification for Single Layer.
//
// Tests the Squadron System Implementation.
// ----------------------------------------------------------------------------

test-setup

select-aadl-file "QuasiTest/packages/QuasiTest.aadl"

// Assertion will stop test at this call
run-agree-single-layer [get-sys-impl "Package Public QuasiTest_public/System Impl Squadron.impl"] | verify-true

is-item-valid "leader equivalence" | verify-false
is-item-valid "form_right assume: not shifting both same" | verify-true
is-item-valid "form_right assume: only shift if none pending" | verify-true
is-item-valid "form_right assume: left shift time" | verify-true
is-item-valid "form_right assume: right shift time" | verify-true
is-item-valid "form_left assume: not shifting both same" | verify-true
is-item-valid "form_left assume: only shift if none pending" | verify-true
is-item-valid "form_left assume: left shift time" | verify-true
is-item-valid "form_left assume: right shift time" | verify-true
check-agree-result "Contract Guarantees" "1 Invalid, 8 Valid" | verify-true

is-item-valid "form_right consistent" | verify-true
is-item-valid "form_left consistent" | verify-true
is-composition-consistent | verify-true
is-component-consistent | verify-true


//run-agree-all-layers [get-sys-impl "Package Public QuasiTest_public/System Impl Squadron.impl"] | verify-true
//is-guarantees-valid | verify-true
//is-composition-consistent | verify-true
//is-component-consistent | verify-true

test-teardown

