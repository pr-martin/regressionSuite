// ----------------------------------------------------------------------------
// Regression Test for GPCA_SW package
//
// 
// Executes the AGREE verification for All Layers.
//
// ----------------------------------------------------------------------------

test-setup

select-aadl-file "GPCA_Controller/packages/GPCA_SW.aadl"
run-agree-all-layers [select-sys-impl "Package Public GPCA_SW_public/System Impl GPCA_SW.Impl"]

check-agree-result "Contract Guarantees" "2 Invalid, 18 Valid"
is-item-valid "Contract Guarantees/PROPERTY 17 : empty_reservoir_implies_audio " | verify-false
is-item-valid "Contract Guarantees/PROPERTY 18 : empty_reservoir_implies_visual_1 " | verify-false

is-component-consistent | verify-true
is-composition-consistent | verify-true
is-item-valid "TLM consistent" | verify-true
is-item-valid "SYS_STAT consistent" | verify-true
is-item-valid "ALARM consistent" | verify-true
is-item-valid "CONFIG consistent" | verify-true
is-item-valid "IM consistent" | verify-true
is-item-valid "OUTPUT consistent" | verify-true

is-item-valid "Verification for TLM/Contract Guarantees/system_start_implies_system_on" | verify-false
is-item-valid "Verification for TLM/Contract Guarantees/not_system_start_implies_system_off" | verify-false
