// ----------------------------------------------------------------------------
// Regression Test for GPCA_SW package
//
// 
// Executes the AGREE verification for Single Layer and All Layers.
//
// ----------------------------------------------------------------------------

test-setup

select-aadl-file "GPCA_Controller/packages/GPCA_SW.aadl"

run-agree-single-layer [select-sys-impl "Package Public GPCA_SW_public/System Impl GPCA_SW.Impl"]

is-component-consistent | verify-true
is-composition-consistent | verify-true
is-item-valid "TLM consistent" | verify-true
is-item-valid "SYS_STAT consistent" | verify-true
is-item-valid "ALARM consistent" | verify-true
is-item-valid "CONFIG consistent" | verify-true
is-item-valid "IM consistent" | verify-true
is-item-valid "OUTPUT consistent" | verify-true


test-teardown

