// ----------------------------------------------------------------------------
// Regression Test for Integer_Toy_Extended example
// 
// ----------------------------------------------------------------------------

test-setup

select-aadl-file "Toy_Example/Integer_Toy_Extended.aadl"


run-agree-single-layer [select-sys-impl "Package Public Integer_Toy_Extended_public/System Impl top_level.Impl"] | verify-true
is-guarantees-valid | verify-true
is-component-consistent | verify-true
is-composition-consistent | verify-true
is-item-valid "A_sub consistent" | verify-true
is-item-valid "B_sub consistent" | verify-true
is-item-valid "C_sub consistent" | verify-true


run-agree-all-layers [select-sys-impl "Package Public Integer_Toy_Extended_public/System Impl top_level.Impl"] | verify-true
is-guarantees-valid | verify-true
is-component-consistent | verify-true
is-composition-consistent | verify-true
is-item-valid "A_sub consistent" | verify-true
is-item-valid "B_sub consistent" | verify-true
is-item-valid "C_sub consistent" | verify-true
check-agree-result "Verification for top_level.Impl" "8 Valid" | verify-true


run-agree-single-layer [select-sys-impl "Package Public Integer_Toy_Extended_public/System Impl above_top.Impl"] | verify-true
is-component-consistent | verify-true
is-composition-consistent | verify-true
is-item-invalid "System output range" | verify-true
is-item-invalid "A_sub assume: A input range" | verify-true
is-item-valid "top_sub assume: System input range " | verify-true
check-agree-result "Contract Guarantees" "2 Invalid, 1 Valid" | verify-true


run-agree-all-layers [select-sys-impl "Package Public Integer_Toy_Extended_public/System Impl above_top.Impl"] | verify-true
is-component-consistent | verify-true
is-composition-consistent | verify-true
is-item-valid "A_sub consistent" | verify-true
is-item-valid "top_sub consistent" | verify-true
is-item-invalid "Contract Guarantees/System output range" | verify-true
is-item-invalid "Contract Guarantees/A_sub assume: A input range" | verify-true
is-item-valid "Contract Guarantees/top_sub assume: System input range " | verify-true
check-agree-result "Contract Guarantees" "2 Invalid, 1 Valid" | verify-true
is-item-valid "Verification for top_sub/Contract Guarantees/System output range" | verify-true
is-item-valid "Verification for top_sub/Contract Guarantees/top_sub.A_sub assume: A input range" | verify-true
is-item-valid "Verification for top_sub/Contract Guarantees/top_sub.B_sub assume: B input range" | verify-true
check-agree-result "Verification for top_sub/Contract Guarantees" "3 Valid" | verify-true
is-item-valid "Verification for top_sub/This component consistent" | verify-true
is-item-valid "Verification for top_sub/Component composition consistent" | verify-true
is-item-valid "Verification for top_sub/A_sub consistent" | verify-true
is-item-valid "Verification for top_sub/B_sub consistent" | verify-true
is-item-valid "Verification for top_sub/C_sub consistent" | verify-true
check-agree-result "Verification for top_sub" "8 Valid" | verify-true
check-agree-result "Verification for above_top.Impl" "2 Invalid, 13 Valid" | verify-true


test-teardown

