// ----------------------------------------------------------------------------
// Regression Test for Real_Toy example
// 
// ----------------------------------------------------------------------------

test-setup

select-aadl-file "Toy_Example/Real_Toy.aadl"

run-agree-single-layer [select-sys-impl "Package Public Real_Toy_public/System Impl top_level.Impl"] | verify-true
is-item-invalid "System output range" | verify-true
is-item-valid "A_sub assume: A input range" | verify-true
is-item-valid "B_sub assume: B input range" | verify-true
check-agree-result "Contract Guarantees" "1 Invalid, 2 Valid" | verify-true
is-composition-consistent | verify-true
is-component-consistent | verify-true
is-item-valid "A_sub consistent" | verify-true
is-item-valid "B_sub consistent" | verify-true
is-item-valid "C_sub consistent" | verify-true


run-agree-all-layers [select-sys-impl "Package Public Real_Toy_public/System Impl top_level.Impl"] | verify-true
is-item-invalid "Contract Guarantees/System output range" | verify-true
is-item-valid "Contract Guarantees/A_sub assume: A input range" | verify-true
is-item-valid "Contract Guarantees/B_sub assume: B input range" | verify-true
check-agree-result "Contract Guarantees" "1 Invalid, 2 Valid" | verify-true
is-composition-consistent | verify-true
is-component-consistent | verify-true
is-item-valid "A_sub consistent" | verify-true
is-item-valid "B_sub consistent" | verify-true
is-item-valid "C_sub consistent" | verify-true

check-agree-result "Verification for top_level.Impl" "1 Invalid, 7 Valid" | verify-true


test-teardown

