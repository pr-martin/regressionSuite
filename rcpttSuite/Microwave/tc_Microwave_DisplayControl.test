// ----------------------------------------------------------------------------
// Regression Test for Microwave example
//
// 
// Executes the AGREE verification for Single Layer and All Layers.
//
// Expect all to be Valid.
// ----------------------------------------------------------------------------

test-setup

select-aadl-file "Microwave/Microwave_SEng5861.aadl"

// Check Realizability
run-agree-realizability [select-sys-impl "Package Public Microwave_SEng5861_public/System Impl Display_Control.Impl"] | verify-true
is-realizability-unknown | verify-true

// Check Monolithic
run-agree-monolithically [select-sys-impl "Package Public Microwave_SEng5861_public/System Impl Display_Control.Impl"] | verify-true
verify-all-results

// Check Single Layer
run-agree-single-layer [select-sys-impl "Package Public Microwave_SEng5861_public/System Impl Display_Control.Impl"] | verify-true
verify-all-results

// Run All Layers
run-agree-all-layers [select-sys-impl "Package Public Microwave_SEng5861_public/System Impl Display_Control.Impl"] | verify-true
verify-all-results

test-teardown

