// ----------------------------------------------------------------------------
// Regression Test for Microwave example
//
// 
// Executes the AGREE verification for Realizability.
//
// ----------------------------------------------------------------------------
test-setup

select-aadl-file "Microwave/Microwave_SEng5861.aadl"

// Test Realizability
run-agree-monolithically [select-outline-item "Package Public Microwave_SEng5861_public/System Mode_Control"] | verify-true
is-realizability-valid

run-agree-monolithically [select-outline-item "Package Public Microwave_SEng5861_public/System Display_Control"] | verify-true
is-realizability-unknown

run-agree-monolithically [select-outline-item "Package Public Microwave_SEng5861_public/System Output_Processing"] | verify-true
is-realizability-valid

run-agree-monolithically [select-outline-item "Package Public Microwave_SEng5861_public/System Microwave_Control"] | verify-true
is-realizability-valid

test-teardown

