// ----------------------------------------------------------------------------
// Regression Test for Microwave example
//
// 
// Executes the AGREE verification for Single Layer and All Layers.
//
// Expect all to be Valid.
// ----------------------------------------------------------------------------

test-setup

select-aadl-file "Microwave/Microwave_SEng5861.aadl"

// CHECK SINGLE LAYER
run-agree-single-layer [select-sys-impl "Package Public Microwave_SEng5861_public/System Impl Mode_Control.Impl"] | verify-true
verify-all-results

run-agree-single-layer [select-sys-impl "Package Public Microwave_SEng5861_public/System Impl Display_Control.Impl"] | verify-true
verify-all-results

run-agree-single-layer [select-sys-impl "Package Public Microwave_SEng5861_public/System Impl Microwave_Control.Impl"] | verify-true
check-agree-result "Contract Guarantees" "2 Invalid, 2 Valid" | verify-true
is-item-valid "Contract Guarantees/heating_element_on implies door_closed_sensor" | verify-true
is-item-invalid "Contract Guarantees/heating element on implies time to cook decreases" | verify-true
is-item-invalid "Contract Guarantees/time to cook = 0 implies heating_element_off" | verify-true
is-item-valid "Contract Guarantees/MC assume: seconds_to_cook is greater than or equal to zero" | verify-true
is-component-consistent | verify-true
is-composition-consistent | verify-true
is-item-valid "MC consistent" | verify-true
is-item-valid "DC consistent" | verify-true
is-item-valid "OP consistent" | verify-true


// RUN ALL LAYERS
run-agree-all-layers [select-sys-impl "Package Public Microwave_SEng5861_public/System Impl Mode_Control.Impl"] | verify-true
verify-all-results

run-agree-all-layers [select-sys-impl "Package Public Microwave_SEng5861_public/System Impl Display_Control.Impl"] | verify-true
verify-all-results

run-agree-all-layers [select-sys-impl "Package Public Microwave_SEng5861_public/System Impl Microwave_Control.Impl"] | verify-true
check-agree-result "Verification for Microwave_Control.Impl" "2 Invalid, 29 Valid" | verify-true
check-agree-result "Contract Guarantees" "2 Invalid, 2 Valid" | verify-true
is-item-valid "Contract Guarantees/heating_element_on implies door_closed_sensor" | verify-true
is-item-invalid "Contract Guarantees/heating element on implies time to cook decreases" | verify-true
is-item-invalid "Contract Guarantees/time to cook = 0 implies heating_element_off" | verify-true
is-item-valid "Contract Guarantees/MC assume: seconds_to_cook is greater than or equal to zero" | verify-true
is-component-consistent | verify-true
is-composition-consistent | verify-true
is-item-valid "MC consistent" | verify-true
is-item-valid "DC consistent" | verify-true
is-item-valid "OP consistent" | verify-true

check-agree-result "Verification for MC" "11 Valid" | verify-true
check-agree-result "Verification for MC/Contract Guarantees" "9 Valid" | verify-true
check-agree-result "Verification for MC/This component consistent" "1 Valid" | verify-true
check-agree-result "Verification for MC/Component composition consistent" "1 Valid" | verify-true

check-agree-result "Verification for DC" "11 Valid" | verify-true
check-agree-result "Verification for DC/Contract Guarantees" "9 Valid" | verify-true
check-agree-result "Verification for DC/This component consistent" "1 Valid" | verify-true
check-agree-result "Verification for DC/Component composition consistent" "1 Valid" | verify-true

test-teardown

